{
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "tabWidth": "4"
            }
        ],
        "no-console": "off",
        "no-underscore-dangle": "off"
    },
    "env": {
        "jest": true
    }
}